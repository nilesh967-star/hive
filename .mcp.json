{
  "mcpServers": {
    "agent-builder": {
      "command": "python",
      "args": ["-m", "framework.mcp.agent_builder_server"],
      "cwd": "core",
      "env": {
        "PYTHONPATH": "../aden-tools/src"
      }
    },
    "aden-tools": {
      "command": "python",
      "args": ["mcp_server.py", "--stdio"],
      "cwd": "aden-tools",
      "env": {
        "PYTHONPATH": "src"
      }
    }
  }
}
